void printNum(int num){
  if (num == 0){
    print_0();
  }
  else if (num == 1){
    print_1();
  }
  else if (num == 2){
    print_2();
  }
  else if (num == 3){
    print_3();
  }
  else if (num == 4){
    print_4();
  }
  else if (num == 5){
    print_5();
  }
  else if (num == 6){
    print_6();
  }
  else if (num == 7){
    print_7();
  }
  else if (num == 8){
    print_8();
  }
  else if (num == 9){
    print_9();
  }
  else{
    delay(250);
  }
  delay(250);
}
